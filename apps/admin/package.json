{
  "name": "@0x219/admin",
  "version": "0.0.0-development",
  "description": "ðŸ“¦ A minimal React Parcel starter template.",
  "type": "module",
  "source": "index.html",
  "scripts": {
    "build": "tsc --noEmit && parcel build",
    "build:report": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "dev": "parcel --port 4900 --no-autoinstall --cache-dir ./.parcel-cache --cert ../../certs/localhost.cert --key ../../certs/localhost.key",
    "preview": "http-server dist --port 4901 -S --cert ../../certs/localhost.cert --key ../../certs/localhost.key",
    "start": "pnpm serve"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "antd": "5.0.6",
    "axios": "1.2.1",
    "clsx": "1.2.1",
    "dayjs": "1.11.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.5",
    "react-transition-group": "4.4.5",
    "superjson": "1.12.0"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-transition-group": "4.4.5",
    "http-server": "14.1.1",
    "typescript": "4.9.4"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    },
    "pseudoClasses": {
      "focusVisible": "focus-visible"
    }
  },
  "alias": {
    "@/*": "./src/$1"
  }
}
